prefix=c:/
includedir=${prefix}/include
libdir=${prefix}/lib

Name: cairo-script-interpreter
Description: script surface backend for cairo graphics library
Version: 1.18.2
Requires: cairo
Requires.private: zlib, pixman-1
Libs: -L${libdir} -lcairo-script-interpreter
Libs.private: -lgdi32 -lmsimg32 -ldwrite -ld2d1 -lwindowscodecs -L${libdir} -lpng16
Cflags: -I${includedir}/cairo
